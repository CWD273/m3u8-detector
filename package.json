{
  "name": "m3u8-fetch-detect",
  "version": "1.0.0",
  "description": "Express server + static client to scrape pages for m3u8 URLs, proxy HLS, and detect what's playing via ID3/PDT.",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --watch server.js --ext js server.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "express": "^4.19.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "hls",
    "m3u8",
    "proxy",
    "id3",
    "express",
    "scraper"
  ],
  "license": "MIT"
}
